{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/svg/**",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ],
        "urls": [
          "https://fonts.googleapis.com/css2?family=Roboto:wght@100&display=swap"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "postsGraphl",
      "urls": ["https://fitnessgeek.ro/backend/graphql/"],
      "version": 1,
      "cacheConfig": {
        "maxSize": 1000,
        "maxAge": "6h",
        "timeout": "10s",
        "strategy": "freshness"
      }
    },
    {
      "name": "postsJson",
      "urls": ["https://fitnessgeek.ro/backend/wp-json/"],
      "version": 1,
      "cacheConfig": {
        "maxSize": 1000,
        "maxAge": "6h",
        "timeout": "10s",
        "strategy": "freshness"
      }
    }
  ]
}
